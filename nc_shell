def receiver() :
    ss = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    ss.bind( ( '10.211.55.19' , 9998) )
    ss.listen(1)
    while(True) :
        conn, addr = ss.accept()
        print conn.recv(1024)
        conn.close()
